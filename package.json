{
  "name": "interactive-quiz-kit",
  "version": "0.1.0",
  "description": "A comprehensive library for creating and playing interactive quizzes with various question types, built with React and TypeScript.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "src/schemas",
    "src/samples"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "start:example": "cd example && npm run start",
    "prepare": "npm run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/interactive-quiz-kit.git"
  },
  "keywords": [
    "quiz",
    "react",
    "typescript",
    "interactive",
    "questions",
    "blockly",
    "scratch",
    "education"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-username/interactive-quiz-kit/issues"
  },
  "homepage": "https://github.com/your-username/interactive-quiz-kit#readme",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitest/ui": "^1.6.0",
    "jsdom": "^24.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup": "^4.42.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@google/genai": "^1.4.0",
    "jszip": "^3.10.1"
  }
}
